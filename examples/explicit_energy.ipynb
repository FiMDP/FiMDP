   "source": [
    "mdp = ConsMDP()\n",
    "mdp.new_states(4)\n",
    "mdp.set_reload(3)\n",
    "mdp.add_action(0, uniform([1,2]), \"α\", 3)\n",
    "mdp.add_action(0, uniform([2,3]), \"β\", 1)\n",
    "mdp.add_action(1, uniform([3]), \"r\", 3)\n",
    "mdp.add_action(2, uniform([3]), \"r\", 1)\n",
    "mdp.add_action(3, uniform([0]), \"s\", 3)\n",
    "mdp.get_Buchi([1,2], 5, True)\n",
    "mdp"
   ]
